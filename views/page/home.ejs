    <script>
        const setMigrationInfo = async() => {
            try{
                const migrationHeight = 24823618;
                const currentHeight = await getCurrentHeight();
                const unixTime = new Date().getTime();
                const dt = new Date(unixTime + ((migrationHeight - currentHeight) * 10000));
                const migrationDate = new Date(dt.getFullYear(), dt.getMonth(), dt.getDate(), dt.getHours(), dt.getMinutes()).toLocaleString("ja-JP");
                document.querySelector("#migration-height").innerHTML = migrationHeight;
                document.querySelector("#current-height").innerHTML = currentHeight;
                document.querySelector("#migration-date").innerHTML = `${migrationDate}頃`;
            } catch (err) {
                document.querySelector("#current-height").innerHTML = `<i class="far fa-face-dizzy"></i>&nbsp;取得できませんでした。`;
                document.querySelector("#migration-date").innerHTML = "";
            }
        }

        (() => {
            setMigrationInfo();
            setInterval(function(){
                setMigrationInfo();
            }, 30000);
        })();
    </script>
